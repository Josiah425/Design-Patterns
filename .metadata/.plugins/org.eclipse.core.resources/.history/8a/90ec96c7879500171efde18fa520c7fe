import java.io.File;

public class Driver {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		File fin;
		FileProcessor fProcessor = new FileProcessor();
		MyArrayList list = new MyArrayList();
		try{
			fin = new File(args[0]);
			String line;
			while(line != null){
				line = fProcessor.readLine(fin);
				int integer = Integer.parseInt(line);
				list.insertSorted(integer);
			}
		}
		catch(Exception e){
			e.printStackTrace();
		}

	}

}
